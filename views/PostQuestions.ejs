<h1 style="text-align: center;">SUBMIT ALL QUESTIONS HERE</h1>
<form action="/QuarkXAcademy/PostQuestions" method="POST">
    <div class="form-group">
      <label for="CorrectOption">Class</label>
      <select class="form-control" id="requirement" name="Class">
              <option value="C3">Class 3</option>
              <option value="C4">Class 4</option>
              <option value="C4">Class 5</option>
              <option value="C4">Class 6</option>
      </select>
    </div>

    <div class="form-group">
      <label for="name">Course</label>
      <input type="text" class="form-control" id="Course" name="Course" placeholder="1" required="true">
    </div>
   
   <div class="form-group">
      <label for="name">Chapter</label>
      <input type="text" class="form-control" id="QuestionID" name="Chapter" placeholder="1" required="true">
    </div>


    <div class="form-group">
      <label for="name">Concept</label>
      <input type="text" class="form-control" id="Concept" name="Concept" placeholder="1" required="true">
    </div>


     <div class="form-group">
      <label for="text">Question</label>
      <input type="text" class="form-control" id="Question" name="Question" placeholder="How many Helicopters are here?" required="true">
    </div>

     <div class="form-group">
      <label for="text">Question Image</label>
      <input type="text" class="form-control" id="email" name="QImage" placeholder="Question Image URL" required="">
    </div>

    <div class="form-group">
      <label for="text">Option A</label>
      <input type="text" class="form-control" id="optA" name="optA" placeholder="This is option A" required="true">
    </div>

   <div class="form-group">
      <label for="text">Option B</label>
      <input type="text" class="form-control" id="optB" name="optB" placeholder="This is option B" required="true">
    </div>

    <div class="form-group">
      <label for="text">Option C</label>
      <input type="text" class="form-control" id="optC" name="optC" placeholder="This is option C" required="true">
    </div>  

    <div class="form-group">
      <label for="text">Option D</label>
      <input type="text" class="form-control" id="optD" name="optD" placeholder="This is option D" required="true">
    </div> 

    
    <div class="form-group">
      <label for="CorrectOption">Correct Option</label>
      <select class="form-control" id="requirement" name="CorrectOption">
              <option value="A">OptA</option>
              <option value="B">OptB</option>
              <option value="C">OptC</option>
              <option value="D">OptD</option>
      </select>
    </div>
    
    <div class="form-group">
      <label for="text">Explanation</label>
      <input type="text" class="form-control" id="Explanation" name="Explanation" placeholder="This is the Explanation" required="true">
    </div> 

     <div class="form-group">
      <label for="text">Explanation Image</label>
      <input type="text" class="form-control" id="AnsImg" name="AnsImg" placeholder="Explanation URL">
    </div> 


     <div class="form-group">
      <label for="text">Score</label>
      <input type="text" class="form-control" id="AnsImg" name="Score" placeholder="Put 1 for initial-scale" required="false">
    </div> 
    
    <div class="form-group">
      <label for="name">Concept Number</label>
      <input type="text" class="form-control" id="ConceptNum" name="ConceptNum" placeholder="ConceptNum" required="true">
    </div>

    <div class="form-group">
      <label for="name">Chapter Number</label>
      <input type="text" class="form-control" id="ChapNum" name="ChapNum" placeholder="ChapNum" required="true">
    </div>

    <button type="submit" class="w3-blue btn btn-default">Submit</button>
  </form>
 
  
