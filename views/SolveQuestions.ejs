  <script>

    var themeFlag = document.getElementById('themeId1').value;
    console.log("Theme", themeFlag);
  
    if (themeFlag=="0"){  //dark mode
      $("#body").css({"background-image": "url(https://abhigyan1996.github.io/QA/QA/1/stars.gif)", "background-repeat" : "repeat-x repeat-y"});
      $("#quesNum").css("color", "orange");
      $("#questionWithImg").css("color", "yellow");
      $("#quesImgContainer").css("border", "0.1px solid #bbb");
      $("#questionNoImg").css("color", "yellow");
      $("#OptionsTxt").css("color", "aqua");
    }
    else if (themeFlag=="1") {  //light mode
      $("#body").css("background", "white");
      $("#quesNum").css("color", "orange");
      $("#questionWithImg").css("color", "black");
      $("#questionNoImg").css("color", "black");
      $("#OptionsTxt").css("color", "indigo");
      $("#quesImgContainer").css("border", "none");
    }
  
    $(document).ready(function(){
        $("#darkMode").click(function(){
          $("#body").css({"background-image": "url(https://abhigyan1996.github.io/QA/QA/1/stars.gif)", "background-repeat" : "repeat-x repeat-y"});
          $("#quesNum").css("color", "orange");
          $("#questionWithImg").css("color", "yellow");
          $("#quesImgContainer").css("border", "0.1px solid #bbb");
          $("#questionNoImg").css("color", "yellow");
          $("#OptionsTxt").css("color", "aqua");
          document.getElementById('themeId1').value = "0";
          document.getElementById('themeId2').value = "0";

        })
  
        $("#lightMode").click(function(){
         // $("#body").css("background-color", "white");
          $("#body").css("background", "white");
          $("#quesNum").css("color", "orange");
          $("#questionWithImg").css("color", "black");
          $("#questionNoImg").css("color", "black");
          $("#OptionsTxt").css("color", "indigo");
          $("#quesImgContainer").css("border", "none");
          document.getElementById('themeId1').value = "1";
          document.getElementById('themeId2').value = "1";
        })
    })
    </script>
  
  
